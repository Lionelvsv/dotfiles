Host *
  UseKeychain yes


#gitlab account
Host gitlab.com-mb10
	HostName gitlab.com
	User git
	IdentityFile ~/.ssh/id_rsa_wagstays

#bitbucket account
Host bitbucket.org-wagstays
	HostName bitbucket.org
	User git
	IdentityFile ~/.ssh/id_rsa_wagstays



